{
  "manifest_version": 2,

  "name": "New space images daily!",
  "description": "This extension allows the user to view new cool space images every day!",
  "version": "1.0",
  "icons": { "128": "Cool_space.jpeg" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Space imgs"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}